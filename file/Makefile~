CC = g++
CFLAGS = -O2   
LDFLAGS = -pthread -lopencv_core -lopencv_imgproc -lopencv_highgui -lopencv_objdetect 

OBJS = filestalk.o csapp.o
#display.o

TEAM = NOBODY
VERSION = 1

all: filestalk

csapp.o: csapp.c 
	$(CC) $(CFLAGS) -c -g csapp.c

filestalk.o: filestalk.cpp
	$(CC) $(CFLAGS) -c -g filestalk.cpp

filestalk: $(OBJS)
	$(CC) $(LDFLAGS) $(OBJS) -o filestalk -lm 
